{
  "compilerOptions": {
    "target": "es6",
    "lib": ["es5", "dom"],
    "types": ["cypress", "node"],
    "jsx": "react-jsx",
    "module": "es6",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
  },
  "include": [
    "src",
    "src/custom.d.ts",
    // "**/*.ts"
  ]
}